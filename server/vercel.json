{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/success", "dest": "/index.html" },
      { "src": "/cancel", "dest": "/index.html" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  